    CONTROL         "",IDC_CLOSE_ACTION_UNLOCKED,"Button",BS_AUTORADIOBUTTON,14,188,300,10
    LTEXT           "",IDC_TRAY_LABEL,7,204,326,10
    CONTROL         "",IDC_TRAY_ON_CLOSE,"Button",BS_AUTOCHECKBOX | WS_TABSTOP,14,218,150,10
    CONTROL         "",IDC_TRAY_ON_MINIMIZE,"Button",BS_AUTOCHECKBOX | WS_TABSTOP,180,218,150,10
    LTEXT           "",IDC_WINDOW_OTHERS_LABEL,7,234,326,10
    CONTROL         "",IDC_NEED_PLUS_BUTTON,"Button",BS_AUTOCHECKBOX | WS_TABSTOP,14,248,150,10
    CONTROL         "",IDC_AUTO_HOOK_WINDOW,"Button",BS_AUTOCHECKBOX | WS_TABSTOP,14,262,300,10
    CONTROL         "",IDC_SHOW_FAIL_NAV_MSG,"Button",BS_AUTOCHECKBOX | WS_TABSTOP,14,276,300,10
