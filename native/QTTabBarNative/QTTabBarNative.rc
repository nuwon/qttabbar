    PUSHBUTTON      "...",IDC_DEFAULT_LOCATION_BROWSE,260,66,70,12
    LTEXT           "",IDC_SESSION_LABEL,7,86,326,10
    CONTROL         "",IDC_SESSION_NEW,"Button",BS_AUTORADIOBUTTON | WS_GROUP | WS_TABSTOP,14,100,300,10
    CONTROL         "",IDC_SESSION_RESTORE_ALL,"Button",BS_AUTORADIOBUTTON,14,114,300,10
    CONTROL         "",IDC_SESSION_RESTORE_LOCKED,"Button",BS_AUTORADIOBUTTON,14,128,300,10
    LTEXT           "",IDC_CLOSE_BUTTON_LABEL,7,146,326,10
    CONTROL         "",IDC_CLOSE_ACTION_WINDOW,"Button",BS_AUTORADIOBUTTON | WS_GROUP | WS_TABSTOP,14,160,300,10
