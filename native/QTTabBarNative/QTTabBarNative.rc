IDR_QTTABBAR_CONTEXT_MENU MENU
BEGIN
    POPUP "&Tabs"
    BEGIN
        MENUITEM "&New Tab", ID_CONTEXT_NEWTAB
        MENUITEM "&Close Tab", ID_CONTEXT_CLOSETAB
        MENUITEM "Restore &Last Closed", ID_CONTEXT_RESTORELASTCLOSED
        MENUITEM SEPARATOR
        MENUITEM "&Refresh", ID_CONTEXT_REFRESH
        MENUITEM "&Options...", ID_CONTEXT_OPTIONS
    END
END

IDD_OPTIONS_DIALOG DIALOGEX 0, 0, 360, 360
STYLE DS_SETFONT | DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "QTTabBar Options"
FONT 9, "Segoe UI", 0, 0, 0x0
BEGIN
    CONTROL         "",IDC_OPTIONS_TAB,"SysTabControl32",WS_TABSTOP | WS_CHILD | WS_CLIPSIBLINGS | WS_CLIPCHILDREN | WS_BORDER,7,7,346,330
    PUSHBUTTON      "Reset",IDC_RESET_BUTTON,102,338,50,14
    DEFPUSHBUTTON   "OK",IDOK,214,338,50,14
    PUSHBUTTON      "Cancel",IDCANCEL,268,338,50,14
    PUSHBUTTON      "Apply",IDC_APPLY_BUTTON,322,338,50,14
END

